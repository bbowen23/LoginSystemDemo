<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<AnchorPane prefHeight="350.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ForgotPasswordController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Forgot password">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="39.0" layoutY="80.0" text="Email" />
      <TextField fx:id="txtEmail" layoutX="39.0" layoutY="97.0" prefHeight="25.0" prefWidth="280.0" />
      <Button fx:id="btnSend" layoutX="68.0" layoutY="164.0" mnemonicParsing="false" onAction="#send" text="Send" />
      <Button fx:id="btnCancel" layoutX="196.0" layoutY="164.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
   </children>
</AnchorPane>
