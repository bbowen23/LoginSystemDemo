<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<AnchorPane prefHeight="350.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ChangePasswordController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Change password">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="38.0" layoutY="76.0" text="Current password" />
      <PasswordField fx:id="txtCurrentPassword" layoutX="38.0" layoutY="93.0" prefHeight="25.0" prefWidth="282.0" />
      <Label layoutX="38.0" layoutY="135.0" text="New password" />
      <PasswordField fx:id="txtNewPassword" layoutX="38.0" layoutY="152.0" prefHeight="25.0" prefWidth="282.0" />
      <Label layoutX="38.0" layoutY="200.0" text="Confirm new password" />
      <PasswordField fx:id="txtConfirmNewPassword" layoutX="38.0" layoutY="217.0" prefHeight="25.0" prefWidth="282.0" />
      <Button fx:id="btnChange" layoutX="59.0" layoutY="275.0" mnemonicParsing="false" onAction="#change" text="Change" />
      <Button fx:id="btnCancel" layoutX="202.0" layoutY="275.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
   </children>
</AnchorPane>
